<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!--指定填充颜色-->
    <solid android:color="#FFFFFF" />

    <!--指定了形状轮廓的粗细与颜色-->
    <stroke
        android:width="1dp"
        android:color="#F69364" />

    <!--指定圆角-->
    <corners
        android:radius="10dp" />

    <!--指定padding-->
    <padding
        android:top="5dp"
        android:bottom="5dp"
        android:left="10dp"
        android:right="10dp" />
</shape>